{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "api/**.csproj"
          ],
          "exclude": [
            "api/AltV.Net.CodeGen*/**.csproj",
            "api/AltV.Net.Client/**.csproj",
            "api/AltV.Net.WebAssembly*/**.csproj",
            "api/AltV.Net*Example/**.csproj",
            "api/AltV.Net*Tests/**.csproj",
            "**/obj/**",
            "**/bin/**"
          ],
          "cwd": ".."
        }
      ],
      "dest": "api",
      "filter": "filterConfig.yml"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md",
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "index.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.yml"
        ]
      }
    ],
    "globalMetadata": {
      "_disableSideFilter": true,
      "_gitContribute": {
        "branch": "master"
      },
      "_gitUrlPattern": "github"
    },
    "template": [
      "default",
      "templates/discordfx"
    ],
    "markdownEngineName": "markdig",
    "dest": "_site"
  }
}
